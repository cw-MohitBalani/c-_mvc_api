<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>AEPLCore.Monitoring</id>
    <version>2.2.1</version>
    <authors>AEPLCore.Monitoring</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A library to monitor rate and latency</description>
    <releaseNotes>v2.2.1
		-----------
		Use 0.0.0.0 for metric server

		v2.2.0
		-----------
		Add support for custom port

		v2.1.0
		-----------
		Add dotnet 5 support

		v2.0.1
		------------------
		Use ConcurrentDictionary for MetricList

		v2.0.0
		------------------
		Added support for .net core 3.1
		Removed support for .net core 2.1/2.0
	v1.3.0
	-----------------
	Added support for automatic metric registration on Consul

    v.1.2.1
    ------------------
    Add support for metrics pusher staging and production env

    v.1.2.0
    ------------------
    Add support for metrics pusher

    v1.1.0
    ------------------
    Added support for MVC. No need to start server in MVC.</releaseNotes>
    <dependencies>
      <group targetFramework=".NETFramework4.5.2">
        <dependency id="AEPLCore.Logging" version="3.0.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client" version="2.2.2" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.MetricPusher" version="0.6.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.MetricServer" version="2.0.2" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.Owin" version="2.1.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETCoreApp3.1">
        <dependency id="AEPLCore.Logging" version="3.0.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client" version="3.1.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.AspNetCore" version="3.1.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.HttpRequestDurations" version="1.0.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.MetricPusher" version="1.1.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.MetricServer" version="3.1.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework="net5.0">
        <dependency id="AEPLCore.Logging" version="3.0.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client" version="3.1.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.AspNetCore" version="3.1.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.HttpRequestDurations" version="1.0.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.MetricPusher" version="1.1.0" exclude="Build,Analyzers" />
        <dependency id="Prometheus.Client.MetricServer" version="3.1.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>